<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Properties>
        <Property name="filename">tasker.log</Property>
    </Properties>
    <Filter type="ThresholdFilter" level="debug"/>
    <Appenders>
        <Console name="ca">
            <PatternLayout pattern="%C{1}.%M %m %ex%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="ca"/>
        </Root>
    </Loggers>
</Configuration>